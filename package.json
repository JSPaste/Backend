{
	"$schema": "https://json.schemastore.org/package.json",
	"name": "@jspaste/backend",
	"private": true,
	"license": "EUPL-1.2",
	"type": "module",
	"scripts": {
		"build": "bun run build:structures && bun run build:structures:dts",
		"build:structures": "bunx pbjs -t static-module -w es6 -o ./src/structures/Structures.js ./src/structures/**/*.proto",
		"build:structures:dts": "bunx pbts -o ./src/structures/Structures.d.ts ./src/structures/Structures.js",
		"dev": "bun run start:watch",
		"fix": "bun run prettier",
		"lint": "bun run lint:tsc",
		"lint:tsc": "bunx tsc --noEmit",
		"prepare": "husky",
		"prettier": "bunx prettier . --write",
		"start": "bun ./src/index.ts",
		"start:watch": "bun ./src/index.ts --watch"
	},
	"dependencies": {
		"@elysiajs/cors": "^0.8.0",
		"@elysiajs/swagger": "^0.8.5",
		"elysia": "^0.8.16",
		"env-var": "^7.4.1",
		"protobufjs": "~7.2.6",
		"protobufjs-cli": "^1.1.2"
	},
	"devDependencies": {
		"@types/bun": "^1.0.5",
		"husky": "^9.0.10",
		"lint-staged": "^15.2.2",
		"prettier": "^3.2.5",
		"prettier-plugin-jsdoc": "^1.3.0",
		"prettier-plugin-packagejson": "^2.4.10",
		"typescript": "~5.3.3"
	}
}
